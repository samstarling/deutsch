    <tr>
      <td>studieren</td>
      <td>studier<%= hi 't' %></td>
    </tr>
    <tr>
      <td>rotieren</td>
      <td>rotier<%= hi 't' %></td>
    </tr>
    <tr>
      <td>marinieren</td>
      <td>marinier<%= hi 't' %></td>
    </tr>
    <tr>
      <td class="row-title" colspan="2">Strong verbs</td>
    </tr>
    <tr>
      <td>kommen</td>
      <td><%= hi 'ge' %>kommen</td>
    </tr>
    <tr>
      <td>fahren</td>
      <td><%= hi 'ge' %>fahren</td>
    </tr>
    <tr>
      <td>gehen</td>
      <td><%= hi 'ge' %>gangen</td>
    </tr>
    <tr>
      <td>singen</td>
      <td><%= hi 'ge' %>sungen</td>
    </tr>
    <tr>
      <td>stehen</td>
      <td><%= hi 'ge' %>standen</td>
    </tr>
    <tr>
      <td class="row-title" colspan="2">Common</td>
    </tr>
    <tr>
      <td>sein</td>
      <td>gewesen</td>
    </tr>
    <tr>
      <td>haben</td>
      <td>gehabt</td>
    </tr>
